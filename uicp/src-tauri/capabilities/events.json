{
  "$schema": "./schema.json",
  "identifier": "events",
  "description": "Allow the main window to listen to Tauri core events (frontend subscribe).",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:webview:default",
    "core:webview:allow-internal-toggle-devtools",
    "core:app:default",
    "core:window:default"
  ]
}
