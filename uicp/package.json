{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "uicp-ui",
    "private": true,
    "version": "0.2.0",
    "license": "Apache-2.0",
    "type": "module",
    "engines": {
        "node": ">=20 <21"
    },
    "packageManager": "npm@10.0.0",
    "scripts": {
        "dev": "vite",
        "build": "node scripts/run-with-rollup-env.mjs npm run build:bundle",
        "build:bundle": "tsc -p tsconfig.build.json && vite build",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "format": "prettier --write .",
        "test": "node scripts/run-with-rollup-env.mjs vitest run",
        "test:watch": "node scripts/run-with-rollup-env.mjs vitest",
        "test:e2e": "playwright test",
        "gen:io": "node scripts/wit-to-ts.mjs",
        "build:components": "node scripts/build-components.mjs",
        "modules:update:csv": "node scripts/update-manifest.mjs --manifest src-tauri/modules/manifest.json --task csv.parse --version 1.2.0 --wasm components/csv.parse/target/wasm32-wasi/release/uicp_task_csv_parse.wasm --filename csv.parse@1.2.0.wasm --copy --outdir src-tauri/modules",
        "modules:update:table": "node scripts/update-manifest.mjs --manifest src-tauri/modules/manifest.json --task table.query --version 0.1.0 --wasm components/table.query/target/wasm32-wasi/release/uicp_task_table_query.wasm --filename table.query@0.1.0.wasm --copy --outdir src-tauri/modules",
        "modules:build:csv": "cargo component build --release --manifest-path components/csv.parse/Cargo.toml",
        "modules:build:table": "cargo component build --release --manifest-path components/table.query/Cargo.toml",
        "modules:build": "npm run modules:build:csv && npm run modules:build:table",
        "modules:publish": "npm run modules:update:csv && npm run modules:update:table",
        "modules:verify": "node scripts/verify-modules.mjs --manifest src-tauri/modules/manifest.json --dir src-tauri/modules",
        "dev:wasm": "node scripts/dev-wasm.mjs",
        "dev:wasm:runtime": "cross-env UICP_MODULES_DIR=src-tauri/modules TAURI_FEATURES=wasm_compute,uicp_wasi_enable,compute_harness tauri dev --features tauri2,wasm_compute,uicp_wasi_enable,compute_harness",
        "tauri:dev": "tauri dev",
        "tauri:build": "tauri build",
        "postinstall": "node scripts/postinstall.cjs"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.0",
        "@liquidglass/react": "^0.1.3",
        "@tauri-apps/api": "^2.8.0",
        "@tauri-apps/plugin-fs": "^2.4.2",
        "clsx": "^2.1.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "react-rnd": "^10.4.10",
        "react-window": "^1.8.10",
        "wit-component": "^0.0.1",
        "zod": "^3.23.8",
        "zustand": "^5.0.0"
    },
    "devDependencies": {
        "@esbuild/win32-x64": "^0.25.10",
        "@playwright/test": "^1.48.2",
        "@rollup/rollup-win32-x64-msvc": "^4.52.4",
        "@tauri-apps/cli": "^2.8.4",
        "@tauri-apps/cli-win32-x64-msvc": "^2.8.4",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^22.7.5",
        "@types/react": "^18.3.10",
        "@types/react-dom": "^18.3.5",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^8.11.0",
        "@typescript-eslint/parser": "^8.11.0",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "esbuild": "^0.25.10",
        "eslint": "^9.13.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "jsdom": "^25.0.1",
        "postcss": "^8.4.49",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.13",
        "typescript": "~5.9.3",
        "vite": "^7.1.7",
        "vitest": "^3.2.4"
    },
    "optionalDependencies": {
        "@rollup/rollup-win32-x64-msvc": "^4.52.4"
    }
}
