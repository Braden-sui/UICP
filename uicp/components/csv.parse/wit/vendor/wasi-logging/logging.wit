package wasi:logging@0.2.0;

/// Simple structured logging interface
@since(version = 0.2.0)
interface logging {
    @since(version = 0.2.0)
    enum level { trace, debug, info, warn, error }

    /// Log a message at a given level
    @since(version = 0.2.0)
    log: func(level: level, context: string, message: string);
}

