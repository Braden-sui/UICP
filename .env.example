########################################
# LLM / Ollama connectivity             #
########################################
# Set to 1 to prefer Ollama Cloud APIs; 0 routes to a local daemon
USE_DIRECT_CLOUD=1
OLLAMA_API_KEY=
OLLAMA_CLOUD_HOST=https://ollama.com
OLLAMA_LOCAL_BASE=http://127.0.0.1:11434/v1
#CHAT_IDLE_TIMEOUT_MS=35000
#FALLBACK_CLOUD_MODEL=

########################################
# Application mode & planner defaults   #
########################################
VITE_UICP_MODE=dev
VITE_DEV_MODE=1
#FOLLOWUP_MAX_DEFAULT=3
#FOLLOWUP_MAX_HARD=5
#ACTOR_BATCH_DEFAULT=50
#ACTOR_BATCH_HARD=200
#APP_WALLCLOCK_MS=5000
#APP_MEM_MB=128
#VITE_PLANNER_TIMEOUT_MS=180000
#VITE_ACTOR_TIMEOUT_MS=180000
#VITE_TASK_SPEC_TIMEOUT_MS=60000
#VITE_CHAT_DEFAULT_TIMEOUT_MS=180000
#VITE_PLANNER_TWO_PHASE=1

########################################
# WIL / interface toggles               #
########################################
#VITE_WIL_ONLY=false
#VITE_WIL_DEBUG=false
#VITE_WIL_MAX_BUFFER_KB=256

########################################
# In-app Network Guard                  #
########################################
VITE_NET_GUARD_ENABLED=1
VITE_NET_GUARD_MONITOR=1
VITE_GUARD_VERBOSE=0
VITE_GUARD_ALLOW_DOMAINS=localhost
VITE_GUARD_ALLOW_IPS=127.0.0.1,::1
VITE_GUARD_ALLOW_IP_RANGES=
VITE_GUARD_ALLOW_PATHS=/api,/ok
#VITE_GUARD_BLOCK_DOMAINS=
#VITE_GUARD_BLOCK_IPS=
#VITE_GUARD_MAX_REQUEST_BYTES=
#VITE_GUARD_MAX_RESPONSE_BYTES=
VITE_GUARD_MAX_REDIRECTS=0
VITE_GUARD_ATTEMPT_SAMPLE=
# Deprecated emergency overrides (prefer policy presets)
#VITE_GUARD_BLOCK_WORKERS=
#VITE_GUARD_BLOCK_SERVICE_WORKER=
#VITE_GUARD_BLOCK_WEBRTC=
#VITE_GUARD_BLOCK_WEBTRANSPORT=

########################################
# Guard rollout & telemetry             #
########################################
VITE_GUARD_ROLLOUT_STAGE=auto
VITE_GUARD_ROLLOUT_MINUTES_MONITOR=30
#VITE_GUARD_FPR_THRESHOLD=0.001
#VITE_GUARD_MIN_ATTEMPTS=50

########################################
# URLHaus reputation service            #
########################################
VITE_URLHAUS_AUTH_KEY=
#VITE_URLHAUS_ENABLED=1
VITE_URLHAUS_MODE=host
#VITE_URLHAUS_API_BASE=https://urlhaus-api.abuse.ch/v1
#VITE_URLHAUS_TIMEOUT_MS=1500
#VITE_URLHAUS_CACHE_TTL_SEC=600
#VITE_URLHAUS_RESPECT_ALLOWS=1
#VITE_URLHAUS_PERSIST=1
#VITE_URLHAUS_PERSIST_KEY=uicp:urlhaus:cache:v1
#VITE_URLHAUS_PERSIST_TTL_SEC=86400
#VITE_URLHAUS_PERSIST_MAX=500

########################################
# Policy presets & safe mode            #
########################################
#UICP_POLICY=open
#UICP_SAFE_MODE=0
#UICP_POLICY_VERSION=
UICP_REQUIRE_TOKENS=0
UICP_JOB_TOKEN_KEY_HEX=
UICP_CACHE_V2=1
#UICP_ADAPTER_V2=0

########################################
